{
  "name": "hubot-rocketchat-boilerplate",
  "version": "0.0.0-develop",
  "description": "An example Hubot demonstrating usage of the Rocket.Chat adaptor.",
  "main": "bin/hubot",
  "repository": "git@github.com:RocketChat/hubot-rocketchat-boilerplate.git",
  "author": "Tim Kinnane <tim.kinnane@rocket.chat>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "hubot",
    "rocketchat",
    "chatbot",
    "chat"
  ],
  "engines": {
    "node": ">=8.x",
    "npm": ">=5.x"
  },
  "files": [
    "build"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "debug": "node --inspect --inspect-brk -r dotenv/config node_modules/hubot/bin/hubot.js -a rocketchat",
    "shell": "node -r dotenv/config node_modules/hubot/bin/hubot.js -a shell",
    "local": "node -r dotenv/config node_modules/hubot/bin/hubot.js -a rocketchat",
    "start": "node  node_modules/hubot/bin/hubot.js --require ./dist/scripts",
    "run": "node -r dotenv/config ./dist/bot/abot.js -a rocketchat",
    "dev": "dotenv -e .env ts-node -- --files node_modules/hubot/bin/hubot.js --require ./src/scripts",
    "clean": "rimraf build",
    "build": "yarn clean && tsc",
    "build:start": "yarn build && yarn start",
    "postinstall": "node ./tools/compile-coffee-deps.js",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@rocket.chat/sdk": "^0.2.9-2",
    "@types/dotenv-defaults": "^2.0.0",
    "@types/hubot": "^3.3.0",
    "coffeescript": "2.5.1",
    "hubot": "^3.3.2",
    "hubot-diagnostics": "^1.0.0",
    "hubot-help": "^1.0.1",
    "hubot-rocketchat": "^2.0.0",
    "hubot-rules": "^1.0.0",
    "hubot-scripts": "^2.17.2",
    "ramda": "^0.27.1",
    "typescript": "^4.0.3",
    "optparse": "1.0.5"
  },
  "devDependencies": {
    "@types/node": "^10.12.7",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "cross-env": "^7.0.2",
    "dotenv": "^5.0.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "lint-staged": "^10.4.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier-eslint-cli": "^5.0.0"
  }
}
